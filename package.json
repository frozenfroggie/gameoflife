{
  "name": "gameoflife",
  "private": "true",
  "version": "1.0.0",
  "description": "Workspace for the Intro to React.js workshop",
  "scripts": {
    "webpack-watch": "webpack --config webpack.dev.config.js --progress --colors --watch",
    "express-server": "nodemon ./server/server.js",
    "dev": "concurrently --kill-others \"npm run webpack-watch\" \"npm run express-server\"",
    "mongo": "mongod --dbpath=/data --smallfiles",
    "tests": "mocha tests/*Test.js",
    "build": "webpack --config webpack.prod.config.js -p --progress --colors",
    "prod": "NODE_ENV=production node server/server.js",
    "postinstall": "webpack --config webpack.prod.config.js -p --progress --colors"
  },
